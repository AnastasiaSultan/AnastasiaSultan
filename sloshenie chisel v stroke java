package lesson8;



public class StringTest {

	public static void main(String[] message) {
		// TODO Auto-generated method stub
		String sb = "12+66+3+1+907";
		String[] sbb = sb.split("\\+");
		int sum = 0;
	
		for (int i=0; i<5; i++) {
			sum += Integer.valueOf(sbb[i]);
		}
		System.err.println("Result=" + sum);
	}
}
